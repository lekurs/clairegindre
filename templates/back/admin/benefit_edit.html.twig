{% extends "admin_layout.html.twig" %}

{%  block head %}
    {{ parent() }}
    {% block title %}
        <title>Administration des familles</title>
    {% endblock %}
{% endblock %}

{% block content %}
    <section>
        <div class="container">
            <div class="col">
                <h1>Administration des cat√©gories de prestations</h1>
            </div>
            <div class="row">
