<!DOCTYPE html>
<html lang="fr">
{% block head %}
    <head>
        <meta charset="UTF-8">
        <!-- Use FOUNDATION CSS -->
        {% block stylesheet %}
            <link href="{{ asset('/css/bootstrap.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/global_styles.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/portfolio.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/large_styles.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/medium_styles.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/small_styles.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/gallery.css') }}" rel="stylesheet" />
            <link href="{{ asset('/css/styles/admin.css') }}" rel="stylesheet" />
            <link href="{{ asset('/fonts/font-awesome.min.css') }}" rel="stylesheet">
            <link href="https://fonts.googleapis.com/css?family=Assistant:200,300,400,600,700,800|Quicksand:300,400,500,700" rel="stylesheet">
        {% endblock %}
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- Si icone -->
        {#<link rel="icon" type="image/ico" href="/images/ico-livre.ico" />#}
        <!--[if IE]><link rel="shortcut icon" type="image/x-icon" href="images/velo-marqueur.png" /><!-->

        {% block meta %}
            <!-- Création des metatags réseaux sociaux -->
            <!-- Metatags FB -->
            <meta property="og:title" content="Claire GINDRE Photographe" />
            <meta property="og:type" content="Claire GINDRE Photographe" />
            <meta property="og:url" content="clairegindre.mclwebservices.com" />
            <meta property="og:description" content="Claire GINDRE - Photographe" />
            <meta property="og:image" content="/css/images/anlor-nico-header.jpg " />

            <!-- Metatag Twitter -->
            <meta name="twitter:card" content="Claire GINDRE Photographe" />
            <meta name="twitter:tittle" content="Claire GINDRE Photographe" />
            <meta name="twitter:description" content="Claire GINDRE - Photographe" />
        {% endblock %}

        {%  block title %}
            <title>Administration</title>
        {% endblock %}

    </head>
{% endblock %}

<body>
{% block header %}
    <header>
        {# NAV MOBILE#}
        {% block mobile %}
            <nav class="d-xl-none d-lg-none mobile-nav" role="navigation">
                <div class="menu">
                    <span class="menu-circle"></span>
                    <a href="#" class="menu-link">
                            <span class="menu-icon">
                                <span class="menu-line menu-line-1"></span>
                                <span class="menu-line menu-line-2"></span>
                                <span class="menu-line menu-line-3"></span>
                            </span>
                    </a>
                </div>

                <div class="menu-overlay">
                    <a href="{{ path('index') }}">home</a>
                    <a href="#hello">hello</a>
                    <a href="{{ path('blog') }}">Photos</a>
                    <a href="{{ path('faq') }}">Infos</a>
                    <a href="#contact">Contact</a>
                    <a href="{{ path('galleriesCustomers') }}">Clients</a>
                </div>
            </nav>
        {% endblock %}
        {# END NAV MOBILE #}

        <div class="row no-gutters">
            <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <div class="row justify-content-center no-gutters nav-desktop">
                    {% block left_menu %}
                        {# LEFT MENU #}
                        <nav class="col-lg-5 col-xl-5 right-menu d-none d-md-none d-sm-none d-lg-block d-xl-block">
                            <div class="row no-gutters justify-content-center align-items-center">
                                <div class="col-lg-6 col-xl-6">
                                    {#ROW RIGHT#}
                                    <div class="row no-gutters nav-border">
                                        <div class="col-lg-12 col-xl-12">
                                            <ul class="nav justify-content-between">
                                                <li class="nav-item"><a class="nav-link" id="nav-link-black-home" href="{{ path('admin') }}">Admin</a></li>
                                                <li class="nav-item"><a class="nav-link nav-link-right" id="nav-link-black-hello" href="{{ path('adminUser') }}">Users</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </nav>
                    {% endblock %}
                    {# LOGO#}
                    {% block logo %}
                        <div class="col-lg-2 col-xl-2 logo-content">
                            <h1><a href="{{ path('index') }}" class="logo-img-color"></a></h1>
                        </div>
                    {% endblock %}
                    {# RIGHT MENU #}
                    {% block right_menu %}
                        <nav class="col-lg-5 col-xl-5 right-menu d-none d-md-none d-sm-none d-lg-block d-xl-block" id="end-nav">
                            <div class="row no-gutters justify-content-center align-items-center">
                                <div class="col-lg-6 col-xl-6">
                                    {#ROW RIGHT#}
                                    <div class="row no-gutters nav-border">
                                        <div class="col-lg-12 col-xl-12">
                                            <ul class="nav justify-content-between">
                                                <li class="nav-item"><a class="nav-link" id="nav-link-black-informations" href="{{ path('index') }}">Retour site</a></li>
                                                <li class="nav-item"><a class="nav-link nav-link-right" id="nav-link-black-customer" href="{{ path('blog') }}">Blog</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </nav>
                        <div class="row no-gutters social-container">
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                                <p><a href="https://www.instagram.com/clairegindre_photographe/" target="_blank"><i class="fa fa-instagram"></i></a></p>
                                <p><a href="https://www.facebook.com/clairegindre.pg/" target="_blank"><i class="fa fa-facebook-f"></i></a></p>
                                <p><a href="https://www.pinterest.fr/ClaireGindrepg/" target="_blank"><i class="fa fa-pinterest-p"></i></a></p>
                            </div>
                        </div>
                    {% endblock %}
                </div>
            </div>
        </div>

        <div class="container-fluid no-gutters">
            <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-xl-12 infos-content"></div>
        </div>

    </header>
{% endblock %}

{% block body %}

    {% block content %}

    {% endblock %}

{% endblock %}
</body>

{% block javascripts %}
    <script src="/js/vendor/jquery-min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="/js/vendor/bootstrap.bundle.js"></script>
    <script src="https://cloud.tinymce.com/stable/tinymce.min.js?apiKey=7g8w49lzy5eyqt1o7hle0gzlmm3c84veiyraf1nwtm9eur9t"></script>
    <script src="{{ asset('js/front/nav-mobile.js') }}"></script>
    <script src="{{ asset('/js/front/menu.js') }}"></script>
{% endblock %}

</html>